*
  box-sizing border-box
  margin 0

html
  font-size 62.5%

$font-size(size=14px)
  font-size size
  [data-dpr="2"] &
    font-size 2 * size
  [data-dpr="3"] &
    font-size 3 * size

/* latin-ext */
@font-face {
  font-family: 'Caveat'
  font-style: normal
  font-weight: 400
  src: local('Caveat'), local('Caveat-Regular'), url(/static/font/mYEqAQGlJunpiMcfKCDxDPesZW2xOQ-xsNqO47m55DA.woff2) format('woff2')
  unicode-range: U+0100-024F, U+1E00-1EFF, U+20A0-20AB, U+20AD-20CF, U+2C60-2C7F, U+A720-A7FF
}
